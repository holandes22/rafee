[uwsgi]
chdir = {{ project_root }}

env = DJANGO_SETTINGS_MODULE={{ django_settings_module }}

home = {{ virtualenv_path }}

# Django's wsgi file
module = {{ project_name }}.wsgi:application

master = true

processes = 10

socket = {{ nginx.socket_file }}

chown-socket = www-data

uid = www-data

gid = www-data

# clear environment on exit
vacuum = true
